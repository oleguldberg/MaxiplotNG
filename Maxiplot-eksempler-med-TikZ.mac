 nolabels:true$
 display2d:false$
 load("mactex.lisp")$
 set_plot_option([run_viewer, false])$
 set_tex_environment_default("","")$
 lbrace:"{"$
 rbrace:"}"$
 file_output_append:false$
 with_stdout("Maxiplot-eksempler-med-TikZ.mxp")$
 file_output_append:true$ 
load("mactex-utilities.lisp")$

